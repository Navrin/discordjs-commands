{
    "name": "simple-discordjs",
    "version": "2.0.0",
    "description": "A command framework for Discord.JS, featuring pattern matching, middlewares and autogenerated help.",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "author": "Navrin Thomas",
    "license": "MIT",
    "watch": {
        "start": {
            "patterns": [
                "src"
            ],
            "extensions": "ts,tsx"
        }
    },
    "scripts": {
        "start": "ts-node ./src/",
        "watch": "npm-watch",
        "build": "tsc -p tsconfig.json",
        "docs": "typedoc ./src --out docs/ --mode file --target ES6 --name \"Simple DiscordJS \" --readme ./README.md --module commonjs",
        "docs:serve": "yarn run docs & ts-node ./docserver.ts"
    },
    "dependencies": {
        "common-tags": "1.8.0",
        "discord.js": "11.4.2",
        "lodash": "4.17.11",
        "mustache": "3.0.1",
        "reflect-metadata": "0.1.12",
        "reverse-mustache": "1.9.0",
        "sqlite3": "4.0.4",
        "string-template": "1.0.0",
        "typeorm": "0.2.9"
    },
    "devDependencies": {
        "@types/cheerio": "0.22.1",
        "@types/common-tags": "1.8.0",
        "@types/escape-string-regexp": "1.0.0",
        "@types/express": "4.16.0",
        "@types/lodash": "4.14.119",
        "@types/mustache": "0.8.32",
        "@types/node": "10.12.12",
        "@types/node-fetch": "2.1.4",
        "@types/reflect-metadata": "0.1.0",
        "@types/sqlite3": "3.1.3",
        "@types/string-template": "1.0.2",
        "@types/table": "4.0.5",
        "express": "4.16.4",
        "npm-watch": "0.5.0",
        "ts-node": "7.0.1",
        "tsc-watch": "1.0.31",
        "tslint-config-airbnb": "5.11.1",
        "tslint-eslint-rules": "5.4.0",
        "typedoc": "0.13.0",
        "typescript": "3.2.2"
    }
}
