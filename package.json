{
  "name": "discordjs-command-helper",
  "version": "0.1.0",
  "description": "A helper class for managing discordjs bot commands.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Navrin Thomas",
  "license": "MIT",
  "watch": {
    "start": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx"
    }
  },
  "scripts": {
    "start": "ts-node ./src/",
    "watch": "npm-watch",
    "build": "tsc -p tsconfig.json",
    "docs": "typedoc ./src --out docs/ --mode file --target ES6 --name \"DiscordJS Command Helper \" --readme ./README.md --module commonjs",
    "docs:serve": "yarn run docs & ts-node ./docserver.ts"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.1",
    "@types/common-tags": "^1.2.5",
    "@types/escape-string-regexp": "^0.0.30",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.64",
    "@types/mustache": "^0.8.29",
    "@types/node": "^7.0.18",
    "@types/node-fetch": "^1.6.7",
    "@types/reflect-metadata": "0.0.5",
    "@types/sqlite3": "^2.2.33",
    "@types/string-template": "^1.0.2",
    "@types/table": "^4.0.2",
    "common-tags": "^1.4.0",
    "discord.js": "^11.1.0",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "mustache": "^2.3.0",
    "reflect-metadata": "^0.1.10",
    "reverse-mustache": "^1.8.0",
    "sqlite3": "^3.1.8",
    "string-template": "^1.0.0",
    "ts-node": "^3.0.4",
    "typeorm": "^0.0.11",
    "typescript": "^2.3.3"
  },
  "devDependencies": {
    "npm-watch": "^0.1.9",
    "tsc-watch": "^1.0.5",
    "tslint-config-airbnb": "^1.1.1",
    "tslint-eslint-rules": "^4.1.0",
    "typedoc": "^0.7.0"
  }
}
